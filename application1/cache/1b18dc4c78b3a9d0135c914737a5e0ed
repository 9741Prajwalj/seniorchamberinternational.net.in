<script>
  $(document).ready(function() {
      setTimeout(function() {
          set_story_box_height();
      }, 1000);
  });

  function set_story_box_height() {
    var max_title = 0;
    $('.swiper-slide .story_heading').each(function() {
        var current_height = parseInt($(this).css('height'));
        if (current_height >= max_title) {
            max_title = current_height;
        }
    });
    $('.swiper-slide .story_heading').css('height', max_title);
  }
</script><script type="text/javascript" src="https://affiliate.siddhrans.in/integration/general_integration"></script>
